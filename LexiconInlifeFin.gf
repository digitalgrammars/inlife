oper mkiAdv = overload {
  mkiAdv  : Str -> S.Adv = \s -> P.mkAdv s ;
  mkiAdv  : S.Adv -> S.Adv = \v -> v ;
  } ;

oper mkiPrep   : Str -> S.Prep  = \s -> mkPrep s ;
oper mkiPrepP  : Str -> S.Prep  = \s -> mkPrep s ;
oper mkiPrepT  : Str -> S.Prep  = \s -> mkPrep s ;
