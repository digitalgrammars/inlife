oper mkiCN = overload {
  mkiCN  : Str -> S.CN = \s -> S.mkCN (mkN s) ;
  mkiCN  : Str -> S.N -> S.CN = \s,n -> S.mkCN (mkN s n) ;
  mkiCN  : Str -> Str -> S.CN = \s,p -> S.mkCN (mkN s p) ;
  mkiCN  : S.N -> S.CN = \a -> S.mkCN a ;
  mkiCN  : M.NForms -> S.CN = \a -> S.mkCN (P.mkN {s = a ; lock_NK = <>}) ;
  mkiCN  : S.A -> S.N -> S.CN = \a,n -> S.mkCN a n ;
  } ;

oper compoundN = overload {
  compoundN : Str -> M.NForms -> S.N = \s,n -> P.mkN s (P.mkN {s = n ; lock_NK = <>}) ;
  } ;
  
oper mkiCNP = overload {
  mkiCNP : Str -> CNP = \s -> lin CNP {cn = S.mkCN (mkN s) ; prep = in_Prep} ;
  mkiCNP : Str -> Str -> CNP = \s,p -> lin CNP {cn = S.mkCN (mkN s) ; prep = mkPrep p} ;
  mkiCNP : N -> CNP = \n -> lin CNP {cn = S.mkCN n ; prep = in_Prep} ;
  mkiCNP : M.NForms -> CNP = \a -> lin CNP {cn = S.mkCN (P.mkN {s = a ; lock_NK = <>}) ; prep = in_Prep} ;
  mkiCNP : N -> Str -> CNP = \n,p -> lin CNP {cn = S.mkCN n ; prep = mkPrep p} ;
  mkiCNP : N -> S.Prep -> CNP = \n,p -> lin CNP {cn = S.mkCN n ; prep = p} ;
  } ;
  
oper mkiNP  : Str -> S.NP = \s -> S.mkNP (mkPN s) ;
oper mkiNPH : Str -> S.NP = \s -> S.mkNP (mkPN s) ;
oper mkiNPP : Str -> S.NP = \s -> S.mkNP (mkPN s) ;
oper mkiNPT : Str -> S.NP = \s -> S.mkNP (mkPN s) ;

oper mkiAP = overload {
  mkiAP  : Str -> S.AP = \s -> S.mkAP (mkA s) ;
  mkiAP  : Str -> Str -> S.AP = \s,t -> S.mkAP (mkA (mkN s t)) ;
  mkiAP  : S.A -> S.AP = \a -> S.mkAP a ;
  } ;

oper mkiV = overload {
  mkiV  : Str -> S.V = \s -> mkV s ;
  mkiV  : S.V -> S.V = \v -> v ;
  mkiV  : M.VForms -> S.V = \v -> P.mkV {s = v ; lock_VK = <>} ;
  } ;

oper mkiV2 = overload {
  mkiV2  : Str -> S.V2 = \s -> mkV2 (mkV s) ;
  mkiV2  : S.V -> S.V2 = \v -> mkV2 v ;
  mkiV2  : P.VK -> S.V2 = \v -> P.mkV2 (P.mkV v) ;
  mkiV2  : M.VForms -> S.V2 = \v -> P.mkV2 (P.mkV {s = v ; lock_VK = <>}) ;
  } ;

--NN oper mkiV2p : Str -> Str -> S.V2 = \s,p -> mkV2 (partV (mkV s) p) ;
