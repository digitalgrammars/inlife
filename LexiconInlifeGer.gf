oper mkiCN = overload {
  mkiCN  : Str -> Gender -> S.CN = \s,g -> S.mkCN (mkN s g) ;
  mkiCN  : Str -> Str -> Gender -> S.CN = \s,p,g -> S.mkCN (mkN s p g) ;
  mkiCN  : S.N -> S.CN = \a -> S.mkCN a ;
  mkiCN  : S.A -> S.N -> S.CN = \a,n -> S.mkCN a n ;
  } ;

oper mkiCNP = overload {
  mkiCNP : Str -> Str -> Gender -> CNP = \s1,s2,g -> lin CNP {cn = S.mkCN (mkN s1 s2 g) ; prep = in_Prep} ;
  mkiCNP : Str -> Str -> Gender -> Str -> Case -> CNP = \s1,s2,g,p,c -> lin CNP {cn = S.mkCN (mkN s1 s2 g) ; prep = mkPrep p c} ;
  mkiCNP : N -> CNP = \n -> lin CNP {cn = S.mkCN n ; prep = in_Prep} ;
  mkiCNP : N -> Str -> Case -> CNP = \n,p,c -> lin CNP {cn = S.mkCN n ; prep = mkPrep p c} ;
  mkiCNP : N -> S.Prep -> CNP = \n,p -> lin CNP {cn = S.mkCN n ; prep = p} ;
  } ;

oper mkiNP  : Str -> S.NP = \s -> S.mkNP (mkPN s) ;

oper mkiAP = overload {
  mkiAP  : Str -> S.AP = \s -> S.mkAP (mkA s) ;
  mkiAP  : S.A -> S.AP = \a -> S.mkAP a ;
  } ;

oper mkiV = overload {
  mkiV  : Str -> S.V = \s -> mkV s ;
  mkiV  : S.V -> S.V = \v -> v ;
  } ;

oper mkiV0  : Str -> S.V  = \s -> mkV s ;

oper mkiV2 = overload {
  mkiV2  : Str -> S.V2 = \s -> mkV2 (mkV s) ;
  mkiV2  : S.V -> S.V2 = \v -> mkV2 v ;
  } ;

oper mkiV2p : Str -> Str -> S.V2 = \s,p -> mkV2 (mkV p (mkV s)) ;
oper mkiV3  : Str -> S.V3 = \s -> mkV3 (mkV s) ;
oper mkiVS  : Str -> S.VS = \s -> mkVS (mkV s) ;
oper mkiVV  : Str -> S.VV = \s -> mkVV (mkV s) ;

oper mkiAdv = overload {
  mkiAdv  : Str -> S.Adv = \s -> P.mkAdv s ;
  mkiAdv  : S.Adv -> S.Adv = \v -> v ;
  } ;

oper mkiPrep   : Str -> Case -> S.Prep  = \s,c -> mkPrep s c ;
oper mkiPrepP = overload {
     mkiPrepP : Str -> Case -> S.Prep  = \s,c -> mkPrep s c ;
     mkiPrepP : Str -> Case -> Str -> S.Prep  = \s1,c,s2 -> mkPrep s1 c s2 ;
};
oper mkiPrepT  : Str -> Case -> S.Prep  = \s,c -> mkPrep s c ;
oper mkiAdV    : Str -> S.AdV   = \s -> lin AdV (P.mkAdv s) ;
oper mkiInterj : Str -> S.Interj = \s -> mkInterj s ;

oper mkiUtt    : Str -> S.Utt = \s -> lin Utt (Pre.ss s) ;

oper mkiCNPL = overload {
  mkiCNPL : Str -> Gender -> S.CN = \s,g -> mkiCN s g ; ----
  mkiCNPL : Str -> Str -> Gender -> S.CN = \s,p,g -> mkiCN s p g ; ----
  mkiCNPL : S.A -> S.N -> S.CN = \ a,n -> mkCN a n ;
  } ;

